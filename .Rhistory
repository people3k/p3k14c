getwd()
library(tidyverse)
RC <- read_csv("radiocarbon_scrubed_v3_2.csv")
View(RC)
RC2 <- RC %>%
group_by(Long, Lat, SiteID, SiteName)
RC2 <- RC %>%
group_by(Long, Lat, SiteID, SiteName) %>%
mutate(SiteInfo = paste(SiteID, SiteName),
Count14C = n()) %>%
ungroup()
RC3 <- RC2[!duplicated(RC2$SiteInfo), ]
RC4 <- RC3 %>%
select(-SiteInfo)
write.csv("RadiocarbonSiteInfo.csv", RC4)
write.csv(RC4, "RadiocarbonSiteInfo.csv", row.names = FALSE)
